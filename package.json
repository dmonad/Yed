{
  "name": "yed",
  "version": "1.0.0",
  "description": "Yet another editor",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "concurrently 'rollup -wc' serve",
    "clean": "rm -rf dist",
    "dist": "npm run clean && mkdir dist && npm run dist:css && npm run dist:css-min && npm run dist:js",
    "dist:js": "PRODUCTION=true rollup -c",
    "dist:css": "cleancss --format beautify -o dist/style.css src/style.css",
    "dist:css-min": "cleancss -O2 -o dist/style.min.css src/style.css"
  },
  "keywords": [
    "editor",
    "prosemirror",
    "yjs"
  ],
  "standard": {
    "ignore": [
      "/dist",
      "/node_modules",
      "/docs"
    ]
  },
  "author": "Kevin Jahns <kevin.jahns@pm.me>",
  "license": "MIT",
  "devDependencies": {
    "@types/prosemirror-model": "^1.7.2",
    "@types/prosemirror-state": "^1.2.3",
    "clean-css-cli": "^4.3.0",
    "codemirror": "^5.49.0",
    "concurrently": "^4.1.2",
    "rollup": "^1.22.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-css-porter": "^1.0.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "y-prosemirror": "^0.1.3",
    "y-protocols": "^0.1.0",
    "y-websocket": "^1.0.4",
    "yjs": "^13.0.0-99"
  },
  "dependencies": {
    "prosemirror-commands": "^1.0.8",
    "prosemirror-example-setup": "^1.0.1",
    "prosemirror-inputrules": "^1.0.4",
    "prosemirror-keymap": "^1.0.1",
    "prosemirror-model": "^1.7.2",
    "prosemirror-state": "^1.2.4",
    "prosemirror-tables": "^0.9.5",
    "prosemirror-view": "^1.11.4"
  }
}
